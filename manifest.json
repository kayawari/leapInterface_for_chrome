{
    "manifest_version": 2,
    "version": "0.1",
    "name": "BrowserMotionExtension",
    "description": "LEAP Motion Chrome Extension",
    "permissions": [
        "tabs", "http://*/*"
    ],
    "background": {
        "scripts": [
            "jquery-2.1.4.min.js",
            "leap.js", 
            "background.js",
            "search.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon19.png",
        "default_title": "BrowserMotion"
    }
}

